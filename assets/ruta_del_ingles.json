  "sections": {
    "pronouns": {
      "title": "Pronouns",
      "items": [
        {
          "id": "pron_subject",
          "label": "I can use subject pronouns (I, you, he, she, it, we, they)."
        },
        {
          "id": "pron_object",
          "label": "I can use object pronouns (me, you, him, her, it, us, them)."
        },
        {
          "id": "pron_poss_adj",
          "label": "I can use possessive adjectives (my, your, his, her, its, our, their)."
        },
        {
          "id": "pron_poss_pron",
          "label": "I can use possessive pronouns (mine, yours, his, hers, ours, theirs)."
        },
        {
          "id": "pron_reflexive",
          "label": "I can use reflexive pronouns (myself, yourself, himself, etc.)."
        }
      ]
    },
    "verbs": {
      "title": "Verbs",
      "items": [
        {
          "id": "verb_regular_ed",
          "label": "I know how to form regular past tense (-ed) and spelling rules."
        },
        {
          "id": "verb_irregular_v123",
          "label": "I can recognize irregular verbs (V1/V2/V3)."
        },
        {
          "id": "verb_third_person_s",
          "label": "I use -s / -es / -ies in the 3rd person singular (he/she/it)."
        },
        {
          "id": "verb_do_does",
          "label": "I use DO/DOES for questions and negatives in the present."
        },
        {
          "id": "verb_did",
          "label": "I use DID for questions and negatives in the past."
        },
        {
          "id": "verb_have_has_pp",
          "label": "I use HAVE/HAS + past participle (present perfect)."
        },
        {
          "id": "verb_passive_basic",
          "label": "I can build the passive voice (be + V3) in basic tenses."
        }
      ]
    },
